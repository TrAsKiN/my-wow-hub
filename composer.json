{
    "name": "traskin/my-wow-hub",
    "description": "A simple WoW Hub in PHP using Battle.net API.",
    "license": "MIT",
    "require": {
        "twig/twig": "^1.0",
        "php-curl-class/php-curl-class": "^4.13",
        "cocur/slugify": "^2.2",
        "silex/silex": "^2.0",
        "symfony/twig-bridge": "^3.1",
        "symfony/asset": "^3.1",
        "symfony/monolog-bridge": "^3.1"
    },
    "require-dev": {
        "symfony/var-dumper": "^3.1",
        "silex/web-profiler": "^2.0"
    },
    "autoload": {
        "psr-4": {"MyWoWHub\\": "src/"}
    },
    "scripts": {
        "post-create-project-cmd": [
            "php -r \"copy('app/config/config.php.dist', 'app/config/config.php');\""
        ]
    }
}
