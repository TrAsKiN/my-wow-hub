Options +FollowSymlinks

<IfModule mod_rewrite.c>
	RewriteEngine on

    RewriteCond %{REQUEST_URI}::$1 ^(/.+)/(.*)::\2$
    RewriteRule ^(.*) - [E=BASE:%1]

    RewriteCond %{HTTP:Authorization} .
    RewriteRule .* - [E=HTTP_AUTHORIZATION:%{HTTP:Authorization}]

    RewriteCond %{ENV:REDIRECT_STATUS} ^$
    RewriteRule ^index\.php(/(.*)|$) %{ENV:BASE}/$2 [R=301,L]

    RewriteCond %{REQUEST_FILENAME} -f
    RewriteRule .? - [L]

#	RewriteRule ^admin/(movie)/([0-9]+),([0-9]+),.*\.html$							%{ENV:BASE}/index.php?admin=true&page=$1&category=$2&paging=$3 [L]
#	RewriteRule ^admin/(category|movie|video)/([0-9]+),(add|update|delete)\.html$	%{ENV:BASE}/index.php?admin=true&page=$1&action=$3&id=$2 [L]
#	RewriteRule ^admin/(category|movie|video)/(add)\.html$							%{ENV:BASE}/index.php?admin=true&page=$1&action=$2 [L]
#	RewriteRule ^admin/(category|movie|video)/$										%{ENV:BASE}/index.php?admin=true&page=$1 [L]
#	RewriteRule ^admin/$															%{ENV:BASE}/index.php?admin=true [L]

#	RewriteRule ^video/([0-9]+),.*\.html$		%{ENV:BASE}/index.php?videoId=$1 [L]
#	RewriteRule ^([0-9]+),([0-9]+),.*\.html$	%{ENV:BASE}/index.php?categoryId=$1&page=$2 [L]
#	RewriteRule ^([0-9]+),.*\.html$				%{ENV:BASE}/index.php?categoryId=$1&page=1 [L]
#	RewriteRule ^result.html$					%{ENV:BASE}/index.php [L]
</IfModule>

<IfModule !mod_rewrite.c>
    <IfModule mod_alias.c>
        RedirectMatch 302 ^/$ /index.php/
    </IfModule>
</IfModule>
